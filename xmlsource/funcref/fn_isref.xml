<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://docbook.org/ns/docbook" xml:id="fn_isref">
      <refmeta>
        <refentrytitle>IsRef
</refentrytitle>
        <refmiscinfo>rdf
</refmiscinfo>
      </refmeta>
      <refnamediv>
        <refname>IsRef
</refname>
        <refpurpose>Returns true if non-blank IRI
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_isref">
          <funcprototype xml:id="fproto_isref">
            <funcdef>
              <function>IsRef
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>iri
</parameter>

   any
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_isref">
        <title>Description
</title>
        <para>Returns true if non-blank IRI
</para>
      </refsect1>
      <refsect1 xml:id="params_isref">
        <title>Parameters
</title>
        <refsect2>
          <title>iri
</title>
          <para>IRI resource
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_isref">
        <title>Return Types
</title>
        <para>Returns 1 if the IRI is non-blank IRI. Otherwise returns 0.
</para>
      </refsect1>
      <refsect1 xml:id="examples_isref">
        <title>Examples
</title>
        <example xml:id="ex_isref">
          <title>Simple example
</title>
          <programlisting>
SQL&gt;SPARQL
SELECT COUNT(DISTINCT (?o))
WHERE
  {
    ?s ?p ?o .
    FILTER ( IsRef(?o))
  }
LIMIT 10

callret-0
76027

</programlisting>
        </example>
      </refsect1>
