      <refnamediv>
        <refname>st_astext
</refname>
        <refpurpose>Returns the well known text (WKT) representation of the geometry.
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_st_astext">
          <funcprototype xml:id="fproto_st_astext">
            <funcdef>
              <function>st_astext
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>g
</parameter>

   any
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_st_astext">
        <title>Description
</title>
        <para>Returns the well known text (WKT) representation of the geometry.
</para>
      </refsect1>
      <refsect1 xml:id="params_st_astext">
        <title>Parameters
</title>
        <refsect2>
          <title>g
</title>
          <para>Geometry point
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_st_astext">
        <title>Return Types
</title>
        <para>Returns varchar.
</para>
      </refsect1>
      <refsect1 xml:id="examples_st_astext">
        <title>Examples
</title>
        <example xml:id="ex_st_astext">
          <title>Simple Use
</title>
          <programlisting>
SQL&gt;SPARQL
SELECT DISTINCT (bif:st_astext(?geo))
WHERE
  {
    ?m geo:geometry ?geo .

  }
LIMIT 10;
callret-0
