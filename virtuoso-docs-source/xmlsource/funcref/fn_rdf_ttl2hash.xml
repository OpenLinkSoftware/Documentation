      <refnamediv>
        <refname>DB.DBA.RDF_TTL2HASH
</refname>
        <refpurpose>Returns a dict of triples in 'long valmode'.
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_rdf_ttl2hash">
          <funcprototype xml:id="fproto_rdf_ttl2hash">
            <funcdef>
              <function>DB.DBA.RDF_TTL2HASH
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>strg
</parameter>

   any
</paramdef>
            <paramdef choice="opt">in 
<parameter>base
</parameter>

   varchar
</paramdef>
            <paramdef choice="opt">in 
<parameter>graph
</parameter>

   varchar
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_rdf_ttl2hash">
        <title>Description
</title>
        <para>Returns a dict of triples in 'long valmode'.
</para>
      </refsect1>
      <refsect1 xml:id="params_rdf_ttl2hash">
        <title>Parameters
</title>
        <refsect2>
          <title>strg
</title>
          <para>text of the resource
</para>
        </refsect2>
        <refsect2>
          <title>base
</title>
          <para>base IRI to resolve relative IRIs to absolute
</para>
        </refsect2>
        <refsect2>
          <title>graph
</title>
          <para>target graph IRI, parsed triples will appear in that graph.
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_rdf_ttl2hash">
        <title>Return Types
</title>
        <para>any
</para>
      </refsect1>
