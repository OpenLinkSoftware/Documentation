      <refsect1 xml:id="desc_http_nt_triple">
        <title>Description
</title>
        <para>Outputs next triple to ses in NT serialization.
</para>
      </refsect1>
      <refsect1 xml:id="params_http_nt_triple">
        <title>Parameters
</title>
        <refsect2>
          <title>env
</title>
          <para>An array of special format:
</para>
          <programlisting>
vector (0, 0, 0);
</programlisting>
          <para>It is used solely as storage for intermediate data that should be freed
      	by the interpreter in case of any error.

</para>
        </refsect2>
        <refsect2>
          <title>arg1
</title>
          <para>Triple subject.
</para>
        </refsect2>
        <refsect2>
          <title>arg2
</title>
          <para>Triple predicate.
</para>
        </refsect2>
        <refsect2>
          <title>arg3
</title>
          <para>Triple object.
</para>
        </refsect2>
        <refsect2>
          <title>ses
</title>
          <para>Session output.
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_http_nt_triple">
        <title>Return Types
</title>
        <para>Any
</para>
      </refsect1>
      <refsect1 xml:id="examples_http_nt_triple">
        <title>Examples
</title>
        <example xml:id="ex_http_nt_triple">
          <title>Simple Use
</title>
          <programlisting>
create procedure dump_rdftriples_to_nt(inout triples any, inout ses any)
{
  declare env any;
  declare tcount, tctr integer;
  tcount := length (triples);
  if (0 = tcount)
    {
      http ('# Empty NT\n', ses);
      return;
    }
  env := vector (0, 0, 0);
  for (tctr := 0; tctr &lt; tcount; tctr := tctr + 1)
    {
      http_nt_triple (env, triples[tctr][0], triples[tctr][1], triples[tctr][2], ses);
    }
}
;
</programlisting>
        </example>
      </refsect1>
