  -->

      <refsect1 xml:id="examples_udt_defines_field">
        <title>Examples
</title>
        <example xml:id="ex_udt_defines_field">
          <title>Simple Use
</title>
          <screen>
select udt_defines_field (new SER_UDT(), 'A');
</screen>
          <para>returns 1</para>
          <screen>
select udt_defines_field (new SER_UDT_SUB(), 'A');
</screen>
          <para>returns 1</para>
          <screen>
select udt_defines_field (new SER_UDT(), 'B');
</screen>
          <para>returns 0;
</para>
        </example>
      </refsect1>
