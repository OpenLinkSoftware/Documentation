<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://docbook.org/ns/docbook" xml:id="fn_strcontains">
      <refmeta>
        <refentrytitle>strcontains
</refentrytitle>
        <refmiscinfo>string
</refmiscinfo>
      </refmeta>
      <refnamediv>
        <refname>strcontains
</refname>
        <refpurpose>Performs substring search
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_strcontains">
          <funcprototype xml:id="fproto_strcontains">
            <funcdef>
              <function>strcontains
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>str
</parameter>

   string
</paramdef>
            <paramdef choice="opt">in 
<parameter>sub
</parameter>

   string
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_strcontains">
        <title>Description
</title>
        <para><function>strcontains
</function>

   performs a
    substring search, returning 1 or 0.
</para>
      </refsect1>
      <refsect1 xml:id="params_strcontains">
        <title>Parameters
</title>
        <refsect2>
          <title>str
</title>
          <para>String to search from.
</para>
        </refsect2>
        <refsect2>
          <title>sub
</title>
          <para>Substring to search for.
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_strcontains">
        <title>Return Types
</title>
        <para>1 if found, 0 if not found.
</para>
      </refsect1>
      <refsect1 xml:id="examples_strcontains">
        <title>Examples
</title>
        <example xml:id="ex_strcontains">
          <title>Simple example
</title>
          <programlisting>
SQL&gt; select strcontains('AbracadabrA','abrA');
strcontains
INTEGER
_______________________________________________________________________________

1

1 Rows. -- 0 msec.

SQL&gt; select strcontains('AbracadabrA','XZ');
strcontains
INTEGER
_______________________________________________________________________________

0

1 Rows. -- 0 msec.
</programlisting>
        </example>
      </refsect1>
