      <refnamediv>
        <refname>get_keyword_ucase
</refname>
        <refpurpose>Find a value in keyword vector (search uppercase)
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_get_keyword_ucase">
          <funcprototype xml:id="fproto_get_keyword_ucase">
            <funcdef>
              <function>get_keyword_ucase
</function>
            </funcdef>
            <paramdef choice="opt"><parameter>keyword
</parameter>

   anything
</paramdef>
            <paramdef choice="opt"><parameter>searched_array
</parameter>

   vector
</paramdef>
            <paramdef choice="opt"><parameter>default
</parameter>

   anything
</paramdef>
            <paramdef choice="opt"><parameter>no_copy
</parameter>

   integer
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_get_keyword_ucase">
        <title>Description
</title>
        <para>Identical to
    <link linkend="fn_get_keyword"><function>get_keyword</function></link>

    except all comparisons  are performed case insensitively.
</para>
      </refsect1>
