<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://docbook.org/ns/docbook" xml:id="fn_get_keyword_ucase">
      <refmeta>
        <refentrytitle>get_keyword_ucase
