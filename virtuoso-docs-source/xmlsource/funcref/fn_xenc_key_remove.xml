      <refnamediv>
        <refname>xenc_key_remove
</refname>
        <refpurpose>This will delete a key from current user's space.
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_xenc_key_remove">
          <funcprototype xml:id="fproto_xenc_key_remove">
            <funcdef>
              <function>xenc_key_remove
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>key_name
</parameter>

   varchar
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_xenc_key_remove">
        <title>Description
</title>
        <para>This will delete a key from current user's space.
</para>
      </refsect1>
      <refsect1 xml:id="params_xenc_key_remove">
        <title>Parameters
</title>
        <refsect2>
          <title>key_name
</title>
          <para>The name of the key.
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="examples_xenc_key_remove">
        <title>Example
</title>
        <example xml:id="ex_xenc_key_remove">
          <title>Simple Use
</title>
          <screen>
xenc_key_remove ('file:dsa.der');</screen>
        </example>
