      <refnamediv>
        <refname>earth_radius
</refname>
        <refpurpose>returns geom.mean of radius of Earth
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_earth_radius">
          <funcprototype xml:id="fproto_earth_radius">
            <funcdef>
              <function>earth_radius
</function>
            </funcdef>
            <paramdef choice="opt"/>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_earth_radius">
        <title>Description
</title>
        <para>returns geom.mean of radius of Earth in kilometers, 6367.43568
</para>
      </refsect1>
      <refsect1 xml:id="ret_earth_radius">
        <title>Return Types
</title>
        <para>Returns any.
</para>
      </refsect1>
      <refsect1 xml:id="examples_earth_radius">
