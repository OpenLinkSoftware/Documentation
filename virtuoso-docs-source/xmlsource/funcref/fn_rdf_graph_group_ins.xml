<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://docbook.org/ns/docbook" xml:id="fn_rdf_graph_group_ins">
      <refmeta>
        <refentrytitle>DB.DBA.RDF_GRAPH_GROUP_INS
</refentrytitle>
        <refmiscinfo>rdf
</refmiscinfo>
      </refmeta>
      <refnamediv>
        <refname>DB.DBA.RDF_GRAPH_GROUP_INS
</refname>
        <refpurpose>Inserts graph into group.
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_rdf_graph_group_ins">
          <funcprototype xml:id="fproto_rdf_graph_group_ins">
            <funcdef>
              <function>DB.DBA.RDF_GRAPH_GROUP_INS
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>group_name
</parameter>

   varchar
</paramdef>
            <paramdef choice="opt">in 
<parameter>graph_uri
</parameter>

   varchar
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_rdf_graph_group_ins">
        <title>Description
</title>
        <para>Inserts graph into a given group.
</para>
      </refsect1>
      <refsect1 xml:id="params_rdf_graph_group_ins">
        <title>Parameters
</title>
        <refsect2>
          <title>group_name
</title>
          <para>The name of the graph group
</para>
        </refsect2>
        <refsect2>
          <title>graph_uri
</title>
          <para>graph uri IRI string or IRI_ID
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_rdf_graph_group_ins">
        <title>Return Types
</title>
        <para>The return value is not specified and may be changed in future versions.
</para>
      </refsect1>
      <refsect1 xml:id="examples_rdf_graph_group_ins">
        <title>Examples
</title>
        <example xml:id="ex_rdf_graph_group_ins">
          <title>Simple example
</title>
          <screen>
SQL&gt;DB.DBA.RDF_GRAPH_GROUP_CREATE ('TestGroup',1);
Done. -- 50 msec.
SQL&gt;DB.DBA.RDF_GRAPH_GROUP_INS ('TestGroup','http://myopenlink.net');
Done. -- 50 msec.
SQL&gt;DB.DBA.RDF_GRAPH_GROUP_INS ('TestGroup','http://demo.openlinksw.com');
Done. -- 50 msec.
</screen>
        </example>
      </refsect1>
      <refsect1 xml:id="seealso_rdf_graph_group_ins">
        <title>See Also
</title>
