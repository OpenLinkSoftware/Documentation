      <refsect1 xml:id="examples_monthname">
        <title>Examples
</title>
        <example xml:id="ex_monthname">
          <title>Simple example
</title>
          <para>Get name of current month.
</para>
          <screen>SQL&gt; select monthname(now());
callret
VARCHAR
