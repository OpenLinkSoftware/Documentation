      <refnamediv>
        <refname>dayofmonth
</refname>
        <refpurpose>get day of month from a datetime
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_dayofmonth_01">
          <funcprototype xml:id="fproto_dayofmonth_01">
            <funcdef>
              <function>dayofmonth
</function>
            </funcdef>
            <paramdef choice="opt">in 
<parameter>dt
</parameter>

   datetime
</paramdef>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_dayofmonth">
        <title>Description
</title>
        <para><function>dayofmonth
</function>

   takes a 
<type>datetime
</type>

   and returns 
    an 
<type>integer
</type>

   containing day of the month represented by the datetime
</para>
      </refsect1>
      <refsect1 xml:id="params_dayofmonth">
        <title>Parameters
</title>
        <refsect2>
          <title>dt
</title>
          <para>A 
<type>datetime
</type>

  .
</para>
        </refsect2>
      </refsect1>
      <refsect1 xml:id="ret_dayofmonth">
        <title>Return Values
</title>
        <para>An 
<type>INTEGER
</type>

   containing the day of month.
</para>
      </refsect1>
