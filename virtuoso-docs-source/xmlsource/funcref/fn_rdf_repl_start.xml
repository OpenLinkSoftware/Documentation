<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://docbook.org/ns/docbook" xml:id="fn_rdf_repl_start">
      <refmeta>
        <refentrytitle>RDF_REPL_START
</refentrytitle>
        <refmiscinfo>repl
</refmiscinfo>
      </refmeta>
      <refnamediv>
        <refname>RDF_REPL_START
</refname>
        <refpurpose>Enables RDF Replication.
</refpurpose>
      </refnamediv>
      <refsynopsisdiv>
        <funcsynopsis xml:id="fsyn_rdf_repl_start">
          <funcprototype xml:id="fproto_rdf_repl_start">
            <funcdef>
              <function>RDF_REPL_START
</function>
            </funcdef>
            <paramdef choice="opt"/>
          </funcprototype>
        </funcsynopsis>
      </refsynopsisdiv>
      <refsect1 xml:id="desc_rdf_repl_start">
        <title>Description
</title>
        <para>
This function is called to enable (start) RDF Publication.

</para>
      </refsect1>
      <refsect1 xml:id="examples_rdf_repl_start">
        <title>Example
</title>
        <example xml:id="ex_rdf_repl_start">
          <title>Enable RDF Publication
</title>
          <screen>
SQL&gt; DB.DBA.RDF_REPL_START();

</screen>
        </example>
      </refsect1>
