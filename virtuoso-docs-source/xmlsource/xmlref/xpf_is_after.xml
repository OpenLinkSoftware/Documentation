      <refsect1 xml:id="xpf_examples_is_after">
        <title>Examples
</title>
        <example xml:id="xpf_ex_is_after">
          <title> 
  </title>
          <para>Find all chapters that have title after table of content
</para>
          <screen>/book/chapter[is-after(toc,title)]
</screen>
