<docbook><section><title>ODSAtomOWLRefExampleBriefcase</title><bridgehead class="http://www.w3.org/1999/xhtml:h2">Briefcase Applications Data Space</bridgehead>
 <programlisting>PREFIX rdf: &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
PREFIX atom: &lt;http://atomowl.org/ontologies/atomrdf#&gt;
PREFIX sioc: &lt;http://rdfs.org/sioc/ns#&gt;
PREFIX sioct: &lt;http://rdfs.org/sioc/types#&gt;
SELECT *
WHERE 
  {
    ?forum rdf:type sioct:Briefcase . 
    ?forum sioc:container_of ?post.
    OPTIONAL{ ?post atom:title ?title } .
    OPTIONAL{ ?post atom:link ?link } .        
    OPTIONAL{ ?post atom:updated ?upd } .        
  }
</programlisting><para> </para>
