<docbook><section><title>ODSFOAFRefExampleBriefcase</title><bridgehead class="http://www.w3.org/1999/xhtml:h3">Briefcase Applications Data Space</bridgehead>
 <programlisting>PREFIX sioc: &lt;http://rdfs.org/sioc/ns#&gt;
PREFIX sioct: &lt;http://rdfs.org/sioc/types#&gt;
PREFIX foaf: &lt;http://xmlns.com/foaf/0.1/&gt;
SELECT DISTINCT ?gender, ?fname, ?post
WHERE 
  {
    ?forum a sioct:Briefcase .
    ?forum sioc:container_of ?post.
    optional { ?post foaf:maker ?maker }.
    optional { ?maker foaf:gender ?gender  } .
    optional { ?maker foaf:family_name ?fname } .
  }
</programlisting><para> </para>
